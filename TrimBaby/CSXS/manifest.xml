<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="7.0" ExtensionBundleId="com.hellaeah.panel" ExtensionBundleVersion="1.0.0"
    ExtensionBundleName="Hellaeah" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <ExtensionList>
        <Extension Id="com.hellaeah.panel" Version="1.0.0" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="AEFT" Version="[13.0,99.9]" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="7.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.hellaeah.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./index.html</MainPath>
                    <CEFCommandLine>
                        <Parameter>--enable-nodejs</Parameter>
                        <Parameter>--mixed-context</Parameter>
                    </CEFCommandLine>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>Hellaeah</Menu>
                    <Geometry>
                        <Size>
                            <Height>200</Height>
                            <Width>300</Width>
                        </Size>
                        <MinSize>
                            <Height>100</Height>
                            <Width>200</Width>
                        </MinSize>
                        <MaxSize>
                            <Height>500</Height>
                            <Width>600</Width>
                        </MaxSize>
                    </Geometry>
                    <Icons>
                        <Icon Type="Normal">./icons/iconNormal.png</Icon>
                        <Icon Type="RollOver">./icons/iconRollover.png</Icon>
                        <Icon Type="DarkNormal">./icons/iconDarkNormal.png</Icon>
                        <Icon Type="DarkRollOver">./icons/iconDarkRollover.png</Icon>
                    </Icons>
                </UI>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>
